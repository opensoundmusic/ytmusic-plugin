{
  "name": "ytmusic-plugin",
  "version": "0.1.1",
  "description": "YouTube Music search and download functionality",
  "author": "MultiX",
  "entry": "index.mjs",
  "enabled": true,
  "dependencies": {
    "express": "^5.1.0",
    "yt-dlp-wrap": "^2.3.12",
    "axios": "^1.13.1",
    "puppeteer": "^24.27.0",
    "node-fetch": "^2.7.0",
    "ytmusic-api": "^5.3.0"
  },
  "routes": [
    {
      "method": "GET",
      "path": "/search",
      "handler": "searchHandler"
    },
    {
      "method": "POST",
      "path": "/download",
      "handler": "downloadHandler"
    }
  ],
  "permissions": ["network", "filesystem", "rabbitmq"]
}
